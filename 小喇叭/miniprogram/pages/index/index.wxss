/**index.wxss**/
.box 
{
  width:90%; 
  height:60px;
  margin: 0 auto; 
}

.login 
{
  width:14%; 
  height:50px;
  display:flex; 
  justify-content:flex-start; 
  margin:10rpx;
  padding:0%;
  float:left;
}

.login::after
{
  border:none;
}

.add
{
  width:14%;
  height:50px;
  display:flex;
  justify-content:flex-end;
  margin:10rpx;
  padding:0%;
  float:right;
}

.add::after
{
  border:none;
}

.Content
{
  display:flex;
  flex-direction:row;
  flex-wrap:wrap;
  font-size:42rpx;
  font-weight:bold;
  line-height:100rpx;
}

.Warn
{
  width:10%; 
  height:40px;
  display:flex;
  float:right;
  margin:1.75%;
  padding:0%;
  margin-top:-80rpx;
}

.Warn::after
{
  border:none;
}

.Remove
{
  width:11%; 
  height:42px;
  display:flex; 
  margin:1.75%;
  padding:0%;
  float:right;
  margin-top:-80rpx;
}

.Remove::after
{
  border:none;
}

.Container
{
  width:100%;
  float:right;
}

page {
  background: #fffcf7;
}
